{
    "include": [ 
        "deployment/system_definition/someip/gh_service/service.json"
    ],
    "package": "srp.apps",
    "adaptive_application": {
        "GhApp": {
            "app": {
                "functional_groups":[
                    "Running",
                    "SafetyMode"
                ],
                "parms": "",
                "logger": {
                    "app_id": "GHSe",
                    "app_des": "",
                    "log_level": "kDebug",
                    "log_mode": "kRemote",
                    "ctx": [
                        {
                            "ctx_id": "ara",
                            "log_level": "kInfo",
                            "ctx_des": "Default ctx for ara"
                        },
                        {
                            "ctx_id": "adiag",
                            "log_level": "kInfo",
                            "ctx_des": "Default ctx for ara::diag"
                        },
                        {
                            "ctx_id": "exec",
                            "log_level": "kDebug",
                            "ctx_des": "Default ctx for ara::exec"
                        }
                    ]
                }
            },
            "provide": [
                {
                    "name": "GhApp as ghService_udp",
                    "on": "udp",
                    "port": "10001",
                    "instance": 1
                },
                {
                    "name": "GhApp as ghService_ipc",
                    "on": "ipc",
                    "instance": 2
                }
            ],
            "require": []
        }
    }
}