        "SysStatService": {
            "app": {
                "functional_groups":[
                    "Running",
                    "SafetyMode"
                ],
                "parms": "",
                "logger": {
                    "app_id": "STAT",
                    "app_des": "",
                    "log_level": "kInfo",
