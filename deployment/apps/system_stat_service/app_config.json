                        {
                            "ctx_id": "exec",
                            "log_level": "kInfo",
                            "ctx_des": "Default ctx for ara::exec"
                        }
                    ]
                }
            },
            "provide": [
                {
                    "name": "SysStatService as SysStatService_ipc",
                    "on": "ipc",
                    "instance": 2
                },
                {
                    "name": "SysStatService as SysStatService_udp",
